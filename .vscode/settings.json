{
  "git.ignoreLimitWarning": true,
  "editor.tabSize": 2,
  "search.exclude": {
    "**/node_modules": false
  },

  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
  },
  "editor.tabCompletion": "on",
  "emmet.triggerExpansionOnTab": true,
  "editor.wrappingIndent": "indent"
}
